	qproperty-minimumSize: 186px 51px;
	qproperty-maximumSize: 186px 51px;
	qproperty-icon: url(./resources/themes/Default/128_white_svg/勾选-m.svg);
	qproperty-iconSize: 29px 29px;
	qproperty-text: Eminent;
}
#desc{
	color: #c1c1c1;
