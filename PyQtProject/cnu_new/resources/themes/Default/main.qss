/* 主窗口 */
#main_frame{
	background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 #343155, stop:1 #3b2e58);
	border-radius: 24px;
}
/* 内容区域 */
#content_frame{
	background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 #3e3a65, stop:1 #473765);
	border-radius: 24px;
}


